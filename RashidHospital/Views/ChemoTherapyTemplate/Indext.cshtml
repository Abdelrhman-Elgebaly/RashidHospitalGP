@model RashidHospital.Models.EmployeeModel

@using (Html.BeginForm("Index", "Home", FormMethod.Post, new { @enctype = "multipart/form-data" }))

{

<div class="text-center">

    <table class="table table-bordered">

        <thead>

            <tr>

                <td>Id</td>

                <td>Name</td>

                <td>Location</td>

                <td>Salery</td>

            </tr>

        </thead>

        <tbody>

            @for (var i = 0; i < Model.EmployeeList.Count; i++)
            {
                <tr>
                    <td>
                        @Html.EditorFor(x => x.EmployeeList[i].DiseaseId)
                    </td>
                    <td>
                        @Html.EditorFor(x => x.EmployeeList[i].ProtocolId)
                    </td>
                </tr>
            }

        </tbody>

    </table>

    <input type="submit" value="Save" />

</div>

}